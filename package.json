{
  "name": "aptly",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android --profile development",
    "build:android-apk": "eas build --platform android --profile development-apk",
    "build:android-local": "expo run:android --variant debug",
    "build:android-preview": "eas build --platform android --profile preview --local",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --collectCoverageFrom='**/*.{ts,tsx}' --collectCoverageFrom='!**/*.d.ts' --collectCoverageFrom='!**/node_modules/**' --collectCoverageFrom='!**/coverage/**' --collectCoverageFrom='!jest.config.js' --collectCoverageFrom='!babel.config.js'",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=__tests__ --testPathIgnorePatterns=integration",
    "test:accessibility": "jest --testNamePattern='accessibility|a11y'",
    "test:performance": "jest --testNamePattern='performance|perf'",
    "test:ci": "jest --ci --coverage --watchAll=false --passWithNoTests",
    "test:debug": "jest --runInBand --detectOpenHandles",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint && npm run test:ci",
    "test:backend": "npx tsx scripts/test-backend-connection.ts",
    "test:mutation": "stryker run",
    "analyze:bundle": "npx @expo/bundle-analyzer",
    "perf:profile": "npx react-native-performance-flipper",
    "e2e:setup": "bash maestro/scripts/setup.sh",
    "e2e:test": "bash maestro/scripts/run-tests.sh",
    "e2e:test:dev": "bash maestro/scripts/run-tests.sh --environment development",
    "e2e:test:staging": "bash maestro/scripts/run-tests.sh --environment staging",
    "e2e:test:auth": "bash maestro/scripts/run-tests.sh --flow maestro/flows/auth",
    "e2e:test:navigation": "bash maestro/scripts/run-tests.sh --flow maestro/flows/navigation",
    "e2e:test:verbose": "bash maestro/scripts/run-tests.sh --verbose"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5.1.4",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "axios": "^1.10.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "expo": "53.0.20",
    "expo-audio": "~0.4.8",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.2.4",
    "expo-document-picker": "~13.1.6",
    "expo-file-system": "~18.1.11",
    "expo-font": "~13.3.2",
    "expo-image": "~2.4.0",
    "expo-image-picker": "~16.1.4",
    "expo-linking": "~7.1.7",
    "expo-local-authentication": "~16.0.5",
    "expo-router": "~5.1.4",
    "expo-secure-store": "~14.2.3",
    "expo-sharing": "~13.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "immer": "^10.1.1",
    "libphonenumber-js": "^1.12.10",
    "lucide-react-native": "^0.511.0",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.56.4",
    "react-native": "0.79.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-mmkv": "^2.12.2",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-swiper": "^1.6.0",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "tailwindcss": "^3.4.17",
    "zod": "^4.0.15",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.0.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.0",
    "prettier": "^3.6.2",
    "react-native-testing-library": "^2.2.0",
    "react-test-renderer": "^19.0.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
